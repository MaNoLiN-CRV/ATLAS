# Atlas Database Configuration
# SQL Server connection settings
DB_HOST=localhost
USERNAME=your_username
PASSWORD=your_password
PORT=1433
DATABASE=your_database_name
# Atlas Performance Collection Configuration
# Collection interval in seconds (how often to collect performance data)
COLLECTION_LAPSE=60

# RAbbitMQ Configuration
RABBITMQ=false

RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
# RabbitMQ configuration
# RabbitMQ exchange and queue names
RABBITMQ_EXCHANGE=atlas_performance
RABBITMQ_QUEUE=atlas_performance_queue
# RabbitMQ routing key
RABBITMQ_ROUTING_KEY=atlas_performance_key
# RabbitMQ connection settings
# RabbitMQ virtual host
RABBITMQ_VHOST=/
# RabbitMQ SSL settings
RABBITMQ_SSL=false
# RabbitMQ heartbeat interval in seconds
RABBITMQ_HEARTBEAT=60
